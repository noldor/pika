{
  "timeout": 10,
  "logs": {
    "text": "infection.log",
    "debug": "debug.log",
    "badge": {
      "branch": "master"
    }
  },
  "source": {
    "directories": [
      "App", "Support"
    ],
    "excludes": ["vendor", "Tests"]
  },
  "mutators": {
    "@default": true,
    "NotIdenticalNotEqual": false,
    "IdenticalEqual": false,
    "CastInt": false,
    "PublicVisibility": false
  },
  "testFramework": "phpunit",
  "phpUnit": {
    "configDir": "./"
  }
}
